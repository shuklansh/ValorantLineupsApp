<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ascent">

    <ImageView
        android:id="@+id/ascentbg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0.8"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/ascent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/aDefault"
        android:layout_width="238dp"
        android:layout_height="72dp"
        android:background="#FBFBFB"
        android:backgroundTint="#FF009688"
        android:text="A site default (heaven)"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ascentbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/ascentbg"
        app:layout_constraintTop_toBottomOf="@+id/bdefbut"
        app:layout_constraintVertical_bias="0.337"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/midcubbybut"
        android:layout_width="247dp"
        android:layout_height="55dp"
        android:layout_marginTop="64dp"
        android:background="#FBFBFB"
        android:backgroundTint="#FF009688"
        android:text="mid cubby anti op"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/ascentbg"
        app:layout_constraintStart_toStartOf="@+id/ascentbg"
        app:layout_constraintTop_toBottomOf="@+id/openadefatk"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bdefbut"
        android:layout_width="247dp"
        android:layout_height="55dp"
        android:layout_marginTop="72dp"
        android:background="#FBFBFB"
        android:backgroundTint="#FF009688"
        android:text="B site default"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/ascentbg"
        app:layout_constraintStart_toStartOf="@+id/ascentbg"
        app:layout_constraintTop_toBottomOf="@+id/midcubbybut"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/openadefatk"
        android:layout_width="251dp"
        android:layout_height="64dp"
        android:layout_marginTop="132dp"
        android:background="#FBFBFB"
        android:backgroundTint="#FF009688"
        android:text="a site default (a-main)"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/ascentbg"
        app:layout_constraintStart_toStartOf="@+id/ascentbg"
        app:layout_constraintTop_toTopOf="@+id/ascentbg"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>